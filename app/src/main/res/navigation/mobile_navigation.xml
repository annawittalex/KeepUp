<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_mytasks">

    <fragment
        android:id="@+id/navigation_mytasks"
        android:name="com.example.keepup.ui.home.HomeFragment"
        android:label="@string/title_my_tasks"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_create"
        android:name="com.example.keepup.ui.dashboard.DashboardFragment"
        android:label="@string/title_create"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_overview"
        android:name="com.example.keepup.ui.notifications.NotificationsFragment"
        android:label="@string/title_overview"
        tools:layout="@layout/fragment_notifications" />
</navigation>